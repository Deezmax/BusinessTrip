{
  "name": "business_trip_backend",
  "version": "0.0.0",
  "scripts": {
    "build": "tsc",
    "start": "node ./dist --env=production",
    "start:dev": "nodemon -L -w src -e ts,json -x ts-node ./src/index.ts",
    "start:debug": "nodemon -L -w src -e ts,json -x node --inspect --nolazy -r ts-node/register/transpile-only ./src/index.ts",
    "test": "nodemon -L --config ./spec/nodemon.json",
    "test:no-reloading": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./spec"
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "command-line-args": "^5.1.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "helmet": "^4.6.0",
    "http-status-codes": "^2.1.4",
    "jet-logger": "^1.0.6",
    "jsonfile": "^6.1.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.12",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.11",
    "@types/jasmine": "^3.7.7",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^15.12.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "eslint": "^7.30.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.0",
    "jasmine": "^3.7.0",
    "mongoose": "^5.13.2",
    "prettier": "^2.3.2",
    "supertest": "^6.1.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.5"
  }
}
